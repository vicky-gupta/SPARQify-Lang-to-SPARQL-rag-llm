    <!-- http://semantic-dicom.org/dcm#SOPInstanceUID -->
    <owl:DatatypeProperty rdf:about="http://semantic-dicom.org/dcm#SOPInstanceUID">
        <rdfs:label>SOP Instance UID</rdfs:label>
        <rdfs:comment>
            SOPInstanceUID is a data property associated with Class Image, representing the unique identifier for the SOP Instance.
        </rdfs:comment>
    </owl:DatatypeProperty>
    

    <!-- http://semantic-dicom.org/dcm#SamplesPerPixel -->
    <owl:DatatypeProperty rdf:about="http://semantic-dicom.org/dcm#SamplesPerPixel">
        <rdfs:comment>
