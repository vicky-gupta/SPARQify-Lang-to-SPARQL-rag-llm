    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://semantic-dicom.org/dcm#WaterEquivalentDiameterCalculationMethodCodeSequence -->
    <owl:ObjectProperty rdf:about="http://semantic-dicom.org/dcm#WaterEquivalentDiameterCalculationMethodCodeSequence">
        <rdfs:comment>
            WaterEquivalentDiameterCalculationMethodCodeSequence is an object property linked to ImageOfCTImage.
        </rdfs:comment>
    </owl:ObjectProperty>
    

    <!-- http://semantic-dicom.org/dcm#containsImage -->
    <owl:ObjectProperty rdf:about="http://semantic-dicom.org/dcm#containsImage">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://semantic-dicom.org/dcm#isImageOf"/>
        <rdfs:domain rdf:resource="http://semantic-dicom.org/dcm#Series"/>
        <rdfs:range rdf:resource="http://semantic-dicom.org/dcm#Image"/>
        <rdfs:comment>
            containsImage is an object property linking a Series to the Image instances it contains.
        </rdfs:comment>
    </owl:ObjectProperty>
    

    <!-- http://semantic-dicom.org/dcm#containsSeries -->
    <owl:ObjectProperty rdf:about="http://semantic-dicom.org/dcm#containsSeries">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://semantic-dicom.org/dcm#isSeriesOf"/>
        <rdfs:domain rdf:resource="http://semantic-dicom.org/dcm#Study"/>
        <rdfs:range rdf:resource="http://semantic-dicom.org/dcm#Series"/>
        <rdfs:comment>
            containsSeries is an object property linking a Study to the Series instances it contains.
        </rdfs:comment>
    </owl:ObjectProperty>
    

    <!-- http://semantic-dicom.org/dcm#isImageOf -->
    <owl:ObjectProperty rdf:about="http://semantic-dicom.org/dcm#isImageOf">
        <rdfs:domain rdf:resource="http://semantic-dicom.org/dcm#Image"/>
        <rdfs:range rdf:resource="http://semantic-dicom.org/dcm#Series"/>
        <rdfs:comment>
            isImageOf is an object property linking an Image to the Series it belongs to. It is the inverse of containsImage.
        </rdfs:comment>
    </owl:ObjectProperty>
    

    <!-- http://semantic-dicom.org/dcm#isSeriesOf -->
    <owl:ObjectProperty rdf:about="http://semantic-dicom.org/dcm#isSeriesOf">
        <rdfs:domain rdf:resource="http://semantic-dicom.org/dcm#Series"/>
        <rdfs:range rdf:resource="http://semantic-dicom.org/dcm#Study"/>
        <rdfs:comment>
            isSeriesOf is an object property linking a Series to the Study it belongs to. It is the inverse of containsSeries.
        </rdfs:comment>
    </owl:ObjectProperty>
    

    <!-- http://semantic-dicom.org/dcm#isStudyOf -->
    <owl:ObjectProperty rdf:about="http://semantic-dicom.org/dcm#isStudyOf">
        <rdfs:domain rdf:resource="http://semantic-dicom.org/dcm#Study"/>
        <rdfs:range rdf:resource="http://semantic-dicom.org/dcm#Patient"/>
        <rdfs:comment>
            isStudyOf is an object property linking a Study to the Patient it belongs to. It is the inverse of hasStudy.
        </rdfs:comment>
    </owl:ObjectProperty>
    


